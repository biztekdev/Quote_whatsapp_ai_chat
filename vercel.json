{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/dashboard.html",
        "dest": "/public/dashboard.html"
      },
      {
        "src": "/login.html",
        "dest": "/public/login.html"
      },
      {
        "src": "/erd.html",
        "dest": "/public/erd.html"
      },
      {
        "src": "/erd-corrected.html",
        "dest": "/public/erd-corrected.html"
      },
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
