{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/dashboard.html",
        "dest": "/public/dashboard.html"
      },
      {
        "src": "/login.html",
        "dest": "/public/login.html"
      },
      {
        "src": "/(api|webhook)/(.*)",
        "dest": "/$1/$2"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
